// R09_NUM05_J.java (non-compliant)
// Uses predictable Random for token generation.
import java.util.Random;

public class R09_NUM05_J {
    public static void main(String[] args) {
        Random rand = new Random();
        int token = rand.nextInt(999999);
        System.out.println("Generated reset token: " + token);
    }
}

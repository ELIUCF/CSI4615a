// R09_NUM05_J.java (compliant)
// Uses SecureRandom for cryptographic token generation.
import java.security.SecureRandom;

public class R09_NUM05_J {
    public static void main(String[] args) {
        SecureRandom secureRand = new SecureRandom();
        int token = secureRand.nextInt(999999);
        System.out.println("Generated secure reset token: " + token);
    }
}
